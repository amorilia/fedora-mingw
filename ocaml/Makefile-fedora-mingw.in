MANEXT=1
RANLIB=i686-pc-mingw32-ranlib
RANLIBCMD=i686-pc-mingw32-ranlib
SHARPBANGSCRIPTS=true
BNG_ARCH=i386
BNG_ASM_LEVEL=1
PTHREAD_LINK=
X11_INCLUDES=
X11_LINK=
DBM_INCLUDES=
DBM_LINK=
TK_DEFS=
TK_LINK=
BYTECC=i686-pc-mingw32-gcc
